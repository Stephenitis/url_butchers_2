%div.container
  %h1 Welcome to URL Butchers!
  %p We can't wait until you sign up
  = form_for(@user) do |f|
    = f.label :email, "Email :"
    = f.email_field :email
    = f.label :password, "Password :"
    = f.password_field :password
    = f.label :password_confirmation, "password_confirmation"
    = f.password_field :password_confirmation
    = f.submit

